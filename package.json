{
  "name": "ejemplodwpcll",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PORT=3000 APP_URL='http://localhost' DEBUG=projnotes node ./dist/bin/www",
    "dev": "NODE_ENV=development PORT=3000 APP_URL='http://localhost' DEBUG=projnotes babel-watch --watch ./server/views ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.config.js --mode development",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "build": "npm run clean && babel ./server --out-dir dist --copy-files",
    "clean": "rm -rf ./dist",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "cleans": "rm -rf ./dist",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3",
    "babel-watch": "^7.8.1",
    "core-js": "^3.32.2",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}

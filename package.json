    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "i": "^0.3.7",
    "materialize-css": "^1.0.0-rc.2",
    "method-override": "^3.0.0",
    "mongoose": "^7.6.3",
    "morgan": "~1.9.1",
    "supports-color": "^9.3.1",
    "validator": "^13.11.0",
    "webpack": "^5.76.1",
    "webpack-dev-middleware": "^6.1.1",

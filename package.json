{
  "name": "ejemplodwpcll",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production DEBUG=projnotes node ./dist/bin/www",
    "dev": "NODE_ENV=development DEBUG=projnotes babel-watch --watch ./server/views ./server/bin/www",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "cleans": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s",
    "clean:logs": "rm -rf ./server/logs/*.log"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.1",
    "connect-flash": "^0.1.1",

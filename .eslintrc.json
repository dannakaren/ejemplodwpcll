{
    "parser": "@babel/eslint-parser",
    "extends":["airbnb-base","prettier"],
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "prettier/prettier": ["error"],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    },
            "globals": {
              "__rootdir": "readonly",
              "M": "readonly"
            }
          }

          